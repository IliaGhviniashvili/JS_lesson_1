let newuser = {
    name: 'giorgi',
    age:  20,
    studentstatus: 'active'
}

if (newuser.age < 18 && newuser.studentstatus == 'active'){
    console.log('hello');
    
}else if(newuser.name == 'levani'){
    console.log('hello levani');
    
}else if(newuser.studentstatus == 'active' || newuser.age < 25){
    console.log('hello world');
    
}else{
    console.log('error');
    
}
